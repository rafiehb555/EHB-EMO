{
  "name": "ehb-jps-tools",
  "version": "1.0.0",
  "description": "EHB JPS Development Tools - Testing, Linting, and Utility Scripts",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "dev": "nodemon",
    "setup": "node scripts/setup.js",
    "deploy": "node scripts/deploy.js",
    "backup": "node scripts/backup.js",
    "monitor": "node scripts/monitor.js",
    "analyze": "node scripts/analyze.js"
  },
  "dependencies": {
    "jest": "^29.0.0",
    "supertest": "^6.3.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "nodemon": "^3.0.0",
    "ts-node": "^10.9.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.0",
    "commander": "^11.1.0",
    "inquirer": "^9.2.0",
    "chalk": "^4.1.2",
    "ora": "^7.0.1",
    "figlet": "^1.6.0",
    "boxen": "^7.1.0",
    "gradient-string": "^2.0.2",
    "axios": "^1.6.0",
    "winston": "^3.11.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "fs-extra": "^11.1.0",
    "glob": "^10.3.0",
    "rimraf": "^5.0.0",
    "copyfiles": "^2.4.1",
    "replace": "^1.2.0",
    "json": "^11.0.0",
    "yaml": "^2.3.0",
    "js-yaml": "^4.1.0",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "exceljs": "^4.4.0",
    "pdf-lib": "^1.17.1",
    "sharp": "^0.32.0",
    "imagemin": "^8.0.1",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-svgo": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/supertest": "^2.0.0",
    "@types/fs-extra": "^11.0.0",
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "testing",
    "linting",
    "formatting",
    "tools",
    "scripts",
    "utilities"
  ],
  "author": "EHB Development Team",
  "license": "MIT"
}
