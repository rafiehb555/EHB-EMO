{
  "name": "emo-business-management",
  "version": "1.0.0",
  "description": "EMO Business Management System - Complete Business Verification and Management Platform",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run dev:ai\" \"npm run dev:blockchain\" \"npm run dev:admin\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev:ai": "cd ai-integration && npm run dev",
    "dev:blockchain": "cd blockchain && npm run dev",
    "dev:admin": "cd admin-panel && npm run dev",
    "build": "npm run build:frontend && npm run build:backend && npm run build:admin",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "build:admin": "cd admin-panel && npm run build",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\" \"npm run start:admin\"",
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && npm start",
    "start:admin": "cd admin-panel && npm start",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && npm test",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:backend": "cd backend && npm run lint",
    "health:check": "node scripts/health-check.js",
    "deploy:dev": "node scripts/deploy-dev.js",
    "deploy:prod": "node scripts/deploy-prod.js",
    "setup": "npm install && cd frontend && npm install && cd ../backend && npm install && cd ../admin-panel && npm install && cd ../ai-integration && npm install && cd ../blockchain && npm install",
    "clean": "npm run clean:node_modules && npm run clean:build",
    "clean:node_modules": "rimraf node_modules frontend/node_modules backend/node_modules admin-panel/node_modules ai-integration/node_modules blockchain/node_modules",
    "clean:build": "rimraf frontend/.next backend/dist admin-panel/.next",
    "reset": "npm run clean && npm run setup"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "concurrently": "^8.0.0",
    "rimraf": "^5.0.0",
    "dotenv": "^16.0.0",
    "winston": "^3.10.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express": "^4.18.0",
    "mongoose": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "express-validator": "^7.0.0",
    "multer": "^1.4.4",
    "sharp": "^0.32.0",
    "tesseract.js": "^4.1.0",
    "pdf-parse": "^1.1.1",
    "openai": "^4.0.0",
    "tensorflow": "^4.0.0",
    "@tensorflow/tfjs-node": "^4.0.0",
    "natural": "^6.0.0",
    "compromise": "^14.0.0",
    "web3": "^4.0.0",
    "ethers": "^6.0.0",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "ipfs-http-client": "^60.0.0",
    "node-cron": "^3.0.0",
    "redis": "^4.6.0",
    "socket.io": "^4.7.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.0",
    "jest": "^29.0.0",
    "supertest": "^6.3.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "@openzeppelin/contracts": "^4.9.0",
    "hardhat": "^2.17.0",
    "@nomiclabs/hardhat-ethers": "^2.2.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "ethereum-waffle": "^4.0.0",
    "chai": "^4.3.0"
  },
  "keywords": [
    "emo",
    "business-management",
    "verification",
    "franchise",
    "blockchain",
    "ai",
    "healthcare",
    "enterprise"
  ],
  "author": "EHB Technologies",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ehb-technologies/emo-business-management.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
} 