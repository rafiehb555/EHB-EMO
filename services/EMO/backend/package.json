    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js"
  },
  "dependencies": {
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.0",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "express-rate-limit": "^6.8.0",
    "express-validator": "^7.0.0",
    "multer": "^1.4.5-lts.1",
    "aws-sdk": "^2.1400.0",
    "nodemailer": "^6.9.0",
    "twilio": "^4.10.0",
    "axios": "^1.6.0",
    "uuid": "^9.0.0",
    "crypto": "^1.0.1",
    "compression": "^1.7.4",
