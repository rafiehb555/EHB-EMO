    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^27.0.0",
    "@babel/cli": "^7.22.0",
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "rimraf": "^5.0.0",
    "prettier": "^3.0.0"
