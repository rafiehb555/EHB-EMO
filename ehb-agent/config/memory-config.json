    "contextSearch": true,
    "sharedKnowledge": true,
    "bestPractices": true,
    "errorSolutions": true,
    "agentKnowledge": true
  },
  "categories": [
    "frontend",
    "backend",
    "blockchain",
    "healthcare",
    "education",
    "finance",
    "general",
    "error-solutions",
    "agent-knowledge",
    "best-practice"
  ],
  "priorities": [
    "low",
    "normal",
    "high",
    "critical"
  ],
  "types": [
    "info",
    "action",
    "decision",
    "error",
    "solution",
    "knowledge",
    "best-practice"
  ]
