  "logFormat": "json",
  "autoSummarizeEvery": 7,
  "shareable": true,
  "autoResume": true,
  "maxSessions": 50,
  "maxEntries": 10000,
  "autoCompress": true,
  "compressionThreshold": 1000,
  "cleanupSettings": {
    "contextsOlderThanDays": 30,
    "sessionsOlderThanDays": 7,
    "sharedMemoryOlderThanDays": 30
  },
  "memoryPaths": {
    "contexts": "../data/memoryLog",
    "sessions": "../data/sessions",
    "sharedMemory": "../data/sharedMemory"
